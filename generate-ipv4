#!/bin/bash
. /usr/local/etc/colors.conf

echo -e "${BLUE}START processing${RESET} ${GREEN}${UNDER}IPv4${RESET} - ${GREEN}${UNDER}ipv4.html${RESET}"
/usr/bin/php -q /root/ionut/vhostsIPv4 > /var/www/html/ipv4.html
echo -e "  ${LIGHT_BLUE}END processing${RESET} ${GREEN}${UNDER}IPv4${RESET} - ${GREEN}${UNDER}ipv4.html${RESET}"
echo -e ""
